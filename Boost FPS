-- TÃ¡c giáº£: GPT-5 | PhiÃªn báº£n Ä‘á»™c láº­p

task.spawn(function()
    local gameService = game
    local workspaceService = gameService.Workspace
    local lightingService = gameService.Lighting
    local terrain = workspaceService.Terrain

    settings().Rendering.QualityLevel = "Level01"

    for _, obj in pairs(gameService:GetDescendants()) do
        if not obj:IsA("Part") and not obj:IsA("UnionOperation") and not obj:IsA("CornerWedgePart") and not obj:IsA("TrussPart") then
            if obj:IsA("Decal") or obj:IsA("Texture") then
                obj.Transparency = 1
            elseif obj:IsA("ParticleEmitter") or obj:IsA("Trail") then
                obj.Lifetime = NumberRange.new(0)
            elseif obj:IsA("Explosion") then
                obj.BlastPressure = 1
                obj.BlastRadius = 1
            elseif obj:IsA("Fire") or obj:IsA("SpotLight") or obj:IsA("Smoke") then
                obj.Enabled = false
            end
        else
            obj.Material = Enum.Material.Plastic
            obj.Reflectance = 0
        end
    end

    print("âœ… Redz FPS Boost: HoÃ n táº¥t! Game Ä‘Ã£ Ä‘Æ°á»£c tá»‘i Æ°u khung hÃ¬nh.")
end)